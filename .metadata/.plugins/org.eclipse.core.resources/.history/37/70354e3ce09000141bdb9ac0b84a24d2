class Shop extends Equipment
{
	Scanner scn=new Scanner(System.in);
	
	public Integer Shop(ArrayList<String> Name,ArrayList<Integer> Price,ArrayList<Integer> Power,ArrayList<Integer> Lucky,ArrayList<Integer> Agility,ArrayList<Integer> Life)
	{
		for(int i=0;i<Name.size();i++)
		{
			System.out.println((i+1)+"¡G"+Name.get(i)+" (¤O¶q¡G"+Power.get(i)+" ©¯¹B¡G"+Lucky.get(i)+" ±Ó±¶¡G"+Agility.get(i)+" ¥Í©R¡G"+Life.get(i)+") "+Price.get(i)+"¤¸");
		}		
		System.out.println("½Ð°Ý§A­n¶R­þ­Ó??(½Ð¿é¤J½s¸¹)(­Y¤£¶R½Ð¿é¤J0)");
		int Buy=scn.nextInt();
		while(Buy<0 || Buy>Name.size())
		{
			System.out.println("¿é¤J¿ù»~!~");
			Buy=scn.nextInt();
		}
		return Buy;		
	}
	
}
public class Shop_Armament_systems_w2 {	
	public static void main(String[] args) {
		Scanner scn=new Scanner(System.in);
		Equipment Eq=new Equipment();
		ArrayList<String> Name=Eq.Name();
		ArrayList<Integer> Price=Eq.Price();
		ArrayList<Integer> Power=Eq.Power();
		ArrayList<Integer> Lucky=Eq.Lucky();
		ArrayList<Integer> Agility=Eq.Agility();
		ArrayList<Integer> Life=Eq.Life();
		ArrayList<String> Items=new ArrayList<String>();
		int Money=2000;
		int power=0;
		int lucky=0;
		int agility=0;
		int life=0;
		int Buy=-1;
		while(Buy!=0)
		{
			Shop Store=new Shop();
			Buy=Store.Shop(Name,Price,Power,Lucky,Agility,Life);
			if(Buy!=0)
			{
				if(Price.get(Buy-1)>Money)
				{
					System.out.println("ª÷¿ú¤£¨¬");
					continue;
				}
				Items.add(Name.get(Buy-1));
				Money-=Price.get(Buy-1);
				power+=Power.get(Buy-1);
				lucky+=Lucky.get(Buy-1);
				agility+=Agility.get(Buy-1);
				life+=Life.get(Buy-1);
			}
			if(Buy==0)
				break;
			System.out.println("§Ú¥Ø«e¾Ö¦³ªº¹D¨ã¡G"+Items);
			System.out.println("§Ú¥Ø«eªº¤O¶q¡G"+power);
			System.out.println("§Ú¥Ø«eªº©¯¹B¡G"+lucky);
			System.out.println("§Ú¥Ø«eªº±Ó±¶¡G"+agility);
			System.out.println("§Ú¥Ø«eªº¥Í©R¡G"+life);
			System.out.println("§Ú¥Ø«e³Ñ¾lª÷¿ú¡G"+Money);
		}
	}
}
